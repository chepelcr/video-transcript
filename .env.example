# Environment Variables Template
# Copy this file to .env.local and fill in your actual values

# Stripe Configuration (Required for payment processing)
VITE_STRIPE_PUBLIC_KEY=pk_test_your_stripe_publishable_key_here
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key_here

# PayPal Configuration (Required for PayPal payments)
PAYPAL_CLIENT_ID=your_paypal_client_id_here
PAYPAL_CLIENT_SECRET=your_paypal_client_secret_here

# Python API Configuration (Required for video transcription)
VITE_PYTHON_API_URL=http://localhost:8000

# Optional: For Stripe subscriptions
STRIPE_PRO_PRICE_ID=price_your_stripe_price_id_here
STRIPE_ENTERPRISE_PRICE_ID=price_your_enterprise_price_id_here