# AWS Configuration (Required for Secrets Manager)
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key

# SQS Configuration
SQS_QUEUE_URL=https://sqs.us-east-1.amazonaws.com/your-account/transcript-queue

# AWS Secrets Manager Integration
# Database credentials are now stored in AWS secret: dev/video-transcript/db
# SMTP credentials are now stored in AWS secret: dev/FrontEnd/ses
# No need to set individual DB/SMTP environment variables

API_BASE_URL=https://your-api-url.example.com

# Payment Provider Secrets (Set in Replit Secrets)
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
PAYPAL_CLIENT_ID=your_paypal_client_id
PAYPAL_CLIENT_SECRET=your_paypal_client_secret

# Frontend URL for CORS (Production)
FRONTEND_URL=https://yourusername.github.io

# External APIs
VITE_PYTHON_API_URL=https://your-python-transcription-api.com

# GitHub Secrets (For GitHub Pages Build)
VITE_STRIPE_PUBLIC_KEY=pk_test_your_stripe_publishable_key
VITE_API_BASE_URL=https://your-replit-app.replit.app